pio run -t cleanall
echo "build samd21 ..."
pio run -e seeed_xiao &>build.seeed_xiao
echo "build samd21 gps..."
pio run -e seeed_xiao_gps &>build.seeed_xiao_gps
echo "build pico ..."
pio run -e pico &>build.pico
echo "build esp32c3 ..."
pio run -e seeed_xiao_esp32c3 &>build.esp32
echo "build pico future ..."
pio run -e pico_future &>build.pifuture
echo "build nrf future ..."
pio run -e sense_future &>build.nrffuture
echo "build nrf future adafruit ..."
pio run -e sense_future_adafruit &>build.nrfadafuture


